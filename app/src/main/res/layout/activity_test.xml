<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <kz.kase.fixtablelayout.FixTableLayout
        android:id="@+id/fixTableLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:fixtable_divider_height="0dp"
        app:fixtable_item_width="100dp"
        app:fixtable_column_1_color="@color/colorStockTableDark"
        app:fixtable_column_2_color="@color/colorStockTable"
        app:fixtable_title_color="#FFFFFFFF" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="reload" />

</LinearLayout>

